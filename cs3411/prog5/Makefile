OBJ = gcc -Wall -c  

all: step1 step2 step3 step4 step5 step6 step7 step8

clean:
	rm -f step1 step2 step3 step4 step5 step6 step7 step8 *.o

step1: step1.o
	gcc step1.o -o step1

step1.o: step1.c
	${OBJ} step1.c

step2: step2.o
	gcc step2.o -o step2

step2.o: step2.c
	${OBJ} step2.c

step3: step3.o
	gcc step3.o -o step3

step3.o: step3.c
	${OBJ} step3.c

