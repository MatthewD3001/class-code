	gcc step4.o -o step4

step4.o: step4.c
	${OBJ} step4.c

step5: step5.o
	gcc step5.o -o step5

step5.o: step5.c
	${OBJ} step5.c

step6: step6.o
	gcc step6.o -o step6

step6.o: step6.c
	${OBJ} step6.c

step7: step7.o
	gcc step7.o -o step7

step7.o: step7.c
	${OBJ} step7.c

step8: step8.o
	gcc step8.o -o step8

step8.o: step8.c
